set -e
. defStone.env
export vers="3.2.15"

rm -rf *.log *.out

newExtent -s product/bin/extent0.dbf $GEMSTONE_NAME

$ROWAN_PROJECTS_HOME/Rowan/platforms/gemstone/topaz/3.2.15/installRowan $GEMSTONE_NAME

startTopaz $GEMSTONE_NAME -l << EOF

  iferr 1 stk
  iferr 2 stack
  iferr 3 exit

# load default group 
  set user UserCurator p swordfish
  login

run
  Rowan projectTools load
    loadProjectFromSpecUrl: 'file:$ROWAN_PROJECTS_HOME/RowanSample6/specs/RowanSample6_default.ston'
    projectRootPath: '$ROWAN_PROJECTS_HOME/RowanSample6/'.
%
commit

  logout

# load globals group
  set user GlobalsCurator p swordfish
  login

run
  Rowan projectTools load
    loadProjectFromSpecUrl: 'file:$ROWAN_PROJECTS_HOME/RowanSample6/specs/RowanSample6_globals.ston'
    projectRootPath: '$ROWAN_PROJECTS_HOME/RowanSample6/'.
%
commit

  exit
EOF
